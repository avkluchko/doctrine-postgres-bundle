{
    "name": "avkluchko/doctrine-postgres-bundle",
    "description": "Doctrine extensions for work with PostgreSQL database",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Klyuchko",
            "email": "avkluchko@yandex.ru"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "autoload": {
        "psr-4": {
            "AVKluchko\\DoctrinePostgresBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AVKluchko\\DoctrinePostgresBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/test-pack": "^1.0",
        "symfony/phpunit-bridge": "^4.3"
    }
}
