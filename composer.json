{
    "name": "avkluchko/postgres-doctrine-extensions",
    "description": "Doctrine extensions for working with PostgreSQL database",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Klyuchko",
            "email": "avkluchko@yandex.ru"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "doctrine/orm": "^2.5"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.4 || ^4.1.12 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "AVKluchko\\PostgresDoctrineExtensions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AVKluchko\\PostgresDoctrineExtensions\\Tests\\": "tests/"
        }
    }
}
